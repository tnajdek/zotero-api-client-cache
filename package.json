{
  "name": "zotero-api-client-cache",
  "version": "0.4.2",
  "description": "A caching layer for zotero-api-client",
  "repository": "https://github.com/tnajdek/zotero-api-client-cache.git",
  "main": "lib/main.js",
  "author": "Tom Najdek",
  "license": "AGPL-3.0",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "test": "nyc mocha --compilers js:babel-register",
    "coverage": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test-watch": "mocha -w --compilers js:babel-register",
    "prepack": "npm run build",
    "postpack": "rimraf lib && rimraf build",
    "build": "rimraf lib/* && rimraf build/* && run-p build:browser build:node",
    "build:node": "mkdir -p lib/ && cd src && babel *.js -d ../lib",
    "build:browser": "mkdir -p build/ && NODE_ENV=browser browserify -s zoteroApiClientCache -d src/main.js -t [babelify] -o build/zotero-api-client-cache.js",
    "start": "mocha -w --require babel-register"
  },
  "files": [
    "build/*.js",
    "lib/*.js"
  ],
  "peerDependencies": {
    "zotero-api-client": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^16.1.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "fetch-mock": "^6.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "timekeeper": "^2.0.0",
    "zotero-api-client": "^0.11.0"
  }
}
