{
  "name": "zotero-api-client-cache",
  "version": "0.4.3",
  "description": "A caching layer for zotero-api-client",
  "repository": "https://github.com/tnajdek/zotero-api-client-cache.git",
  "main": "lib/main.js",
  "author": "Tom Najdek",
  "license": "AGPL-3.0",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "test": "nyc mocha --require babel-core/register",
    "coverage": "nyc --reporter=html --reporter=text mocha --require babel-core/register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test-watch": "mocha -w --require babel-core/register",
    "prepack": "npm run build",
    "postpack": "rimraf lib && rimraf build",
    "lint": "eslint \"src/*.js\" || true",
    "clean": "rimraf build",
    "build": "rimraf lib/* && rimraf build/* && run-p browserify build:node",
    "build:node": "mkdir -p lib/ && cd src && babel *.js -d ../lib",
    "browserify": "mkdir -p build/ && NODE_ENV=production browserify -s zoteroApiClientCache src/main.js -t [babelify] -g [ envify --NODE_ENV production ] -g uglifyify | uglifyjs --compress --mangle > build/zotero-api-client-cache.js",
    "start": "mocha -w --require babel-core/register"
  },
  "files": [
    "build/*.js",
    "lib/*.js"
  ],
  "peerDependencies": {
    "zotero-api-client": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "envify": "^4.1.0",
    "fetch-mock": "^6.4.2",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.8.0",
    "rimraf": "^2.6.2",
    "timekeeper": "^2.1.2",
    "uglify-js": "^3.3.25",
    "uglifyify": "^5.0.0",
    "zotero-api-client": "^0.12.0"
  }
}
